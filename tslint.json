{
  "defaultSeverity": "warning",
  "extends": ["tslint-config-prettier"],
  "rulesDirectory": ["tslint-plugin-prettier"],
  "linterOptions": {
    "exclude": ["node_modules/**"]
  },
  "rules": {
    "indent": [true, "spaces", 2],
    "prettier": true
  },
  "jsRules": {
    "no-empty": true
  }
}
